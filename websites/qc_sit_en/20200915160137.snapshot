});

/* Fonction pour créer le tableau selon les données reçues */
function generateHtmlTableEN(data, nomTableau) {
    var html = '<table  class="table table-condensed table-hover table-striped">';
    /* S'il n'y a pas de données, on ne fait pas le tableau*/
    if(typeof(data[0]) === 'undefined') {
      return null;
    } else {
      /*Sinon, on passe chacune des rangées du CSV*/
      $.each(data, function( index, row ) {
        //Si c'est la première rangée, on ajoute l'entête du tableau avec les TH
        if(index == 0) {
          html += '<thead>';
          html += '<tr>';
          /* Pour chacune des cellules de la première rangée, on ajoute les TH*/
          $.each(row, function( index, colData ) {
            if (index > 0) {
              html += '<th style="text-align:right;">';
            } else {
              html += '<th>';
            }
            html += colData;
            html += '</th>';
          });
          html += '</tr>';
          html += '</thead>';
          html += '<tbody>';
        } else {
          /* Pour toutes les autres rangées du csv, on ajoute les rangées du tableau avec données TD */
          html += '<tr>';
          $.each(row, function( index, colData ) {
            /* Création de la cellule de données */
            if (index > 0) {
              html += '<td style="text-align:right;">';
              html += colData.replace(/ /g, "&nbsp;");
            } else {
              html += '<td>';
              html += colData;
            }
            html += '</td>';
          });
          html += '</tr>';
        }
      });
      html += '</tbody>';
      html += '</table>';
      /* Ajout au DOM du tableau créé*/
      $('#' + nomTableau).append(html);
    }
}

/* Fonction pour créer le tableau selon les données reçues */
function genererSituationQuebecEn(data, nomTableau) {
  var html = '';
  if(typeof(data[0]) === 'undefined') {
    return null;
  } else {
    $.each(data, function( index, row ) {
      //Si c'est la première rangée, on ajoute l'entête du tableau avec les TH
      if(index == 0) {
        html += '<div class="frame frame-default frame-type-textmedia frame-layout-0 frame-space-before-medium"><div class="ce-textpic ce-left ce-above"><div class="ce-bodytext"><div class="zoneMinMarginPadding"><h2>Situation in Québec</h2><p class="petitTexte">';
        html += row[1];
        html += '</p><hr></div></div></div></div>';
        html += '<div class="row">';
      } else {
        /* Pour toutes les autres rangées du csv, on ajoute les rangées du tableau avec données TD */
        html += '<div class="  col-md-3  tableauCovid-19"><div id="c50741" class=" frame frame-default frame-type-textmedia frame-layout-0"><div class="ce-textpic ce-left ce-above"><div class="ce-bodytext">';
        $.each(row, function( index, colData ) {
          html += '<p>';
          html += colData;
          html += '</p>';
        });
        html += '</div></div></div></div>';
      }
    });
    html += '</div>';
    html += '<div class=" frame frame-default frame-type-textmedia frame-layout-0 frame-space-after-medium"><div class="ce-textpic ce-left ce-above"><div class="ce-bodytext"><hr></div></div></div>';
    $('#' + nomTableau).append(html);
  }
}  
